<% include partials/header.ejs %>
<h1 class="col-xs-offset-2">Contact Me</h1>
<br>
<form class="form-horizontal" action="/thanks" method="post">
  <div class="form-group">
    <label class="control-label col-sm-2" for="name">Name</label>
    <input class="col-xs-4" type="text" id="nane" placeholder="John Doe" required>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="email">Email</label>
    <input class="col-xs-4" type="email" id="" placeholder="email@email.com" required>
  </div>
  <div class="form-group">
    <label for="message" class="control-label col-sm-2">Message</label>
    <textarea name="message" class="col-xs-4" id="message" placeholder="Hey Sakura, just wanted to let you know that while your website is somewhat bland, your bootstrap looks really amazing  " required cols="40" rows="5" ></textarea>
  </div>
  <div class="form-group">
    <div class="col-xs-offset-2 col-xs-2">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div>
</form>
<% include partials/footer.ejs %>
